#!/bin/bash

# declare -a packages=("p7zip" "audacity" "firefox" "gimp" "handbrake" "inkscape" "krita" "libreoffice" "tux-paint" "vlc" "chromium-browser")
declare -a packages=(libreoffice zoom skype brave bitwarden googlchrome)

for package in ${packages[@]}; do
    echo "******************" 
    echo $package
    echo "******************"
    echo install  $package
    ./Installomator.sh $package DEBUG=0 NOTIFY=silent
done
